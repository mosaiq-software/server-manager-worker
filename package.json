{
    "name": "@mosaiq/nsm-worker",
    "version": "1.0.0",
    "description": "",
    "main": "src/index.ts",
    "type": "module",
    "scripts": {
        "start": "tsx watch src/index.ts",
        "lint": "eslint .",
        "types": "tsc --noemit",
        "common:prod": "date && npm un @mosaiq/nsm-common && npm i @mosaiq/nsm-common@latest",
        "common:dev": "npm un @mosaiq/nsm-common && npm i ../server-manager/common/dist",
        "deploy": "npm run executor:start; docker compose -p nsm-worker up --build -d",
        "executor:start": "npm run executor:stop; pm2 start nsmExecHandle.mjs",
        "executor:stop": "pm2 delete nsmExecHandle.mjs"
    },
    "author": "",
    "dependencies": {
        "@mosaiq/nsm-common": "../server-manager/common/dist",
        "cors": "^2.8.5",
        "dotenv": "^16.4.5",
        "express": "^4.21.1",
        "query-string": "^9.1.1"
    },
    "devDependencies": {
        "tsx": "^4.19.2",
        "typescript": "^5.6.3",
        "@eslint/js": "^9.21.0",
        "@types/cors": "^2.8.17",
        "@types/express": "^5.0.0",
        "@types/node": "^22.10.2",
        "eslint": "^9.21.0",
        "globals": "^16.0.0",
        "typescript-eslint": "^8.24.1",
        "prettier": "^3.5.3"
    }
}
